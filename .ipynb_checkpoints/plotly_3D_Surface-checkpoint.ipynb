   "source": [
    "import plotly.graph_objects as go\n",
    "import numpy as np\n",
    "\n",
    "x = y = np.linspace(-3,3,50)\n",
    "x,y = np.meshgrid(x,y);\n",
    "Z = np.sin(x**2 + y**2)\n",
    "\n",
    "fig = go.Figure(data=[go.Surface(z=Z, colorscale='Viridis')])\n",
    "\n",
    "fig.update_layout(title=\"3D surface\"); \n",
    "fig.show()"
   ]
