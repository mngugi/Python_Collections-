    "import turtle\n",
    "\n",
    "turt = turtle.Turtle()\n",
    "\n",
    "colors = [\"purple\",\"green\", \"blue\",\"red\",\"yellow\",\"cyan\"]\n",
    "\n",
    "for i in range(6):\n",
    "    turt.color(colors[i])\n",
    "    for _ in range(5):\n",
    "        turt.forward(100)\n",
    "        turt.right(72)\n",
    "    turt.right(60)\n",
    "\n",
    "turtle.done()\n",
    "    "
